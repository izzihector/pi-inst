<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>

        <record id="starter_res_users_view_form_pivotino" model="ir.ui.view">
            <field name="name">starter.res.users.view.form.pivotino</field>
            <field name="model">res.users</field>
            <field name="inherit_id" ref="pivotino_base.res_users_view_form_pivotino"/>
